<p>会員の方はこちらからログイン</p>

<%= form_with model: @customer, url: session_path(resource_name), local: true do |f| %>
    <table>
      <tr>
        <td>メールアドレス<td>
        <td><%= f.email_field :email, autofocus: true %></td>
      </tr>
      <tr>
        <td>パスワード<td> 
        <td><%= f.password_field :password%></td>
      </tr>
    </table>
    

  <div class="actions">
    <%= f.submit "ログイン" ,class: "btn btn-primary"%>
  </div>
<% end %>

<p>登録がお済みでない方</p>
<%= link_to "こちら", new_customer_registration_path%>

<%= render "customers/shared/links" %>
